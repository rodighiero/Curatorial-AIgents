                <dd>
                                                                 <a href=https://www.harvardartmuseums.org/collections/person/53419>Iorwerth Grey Lloyd</a>, British                                                                                                                                                                      (1845-1903)
                                            <br>
                                    </dd>
            
                            <dt>Title</dt>
                <dd>Scrapbook</dd>
            
                                                                                                            
                            <dt>Classification</dt>
                <dd>Photographs</dd>
                                        <dt>Work Type</dt>
                <dd>
                                                            album                                                      </dd>
                                        <dt>Date</dt>
                <dd>1864-1871</dd>
                                        <dt>Places</dt>
                <dd>
                                                            Creation Place: Europe, United Kingdom                                                      </dd>
                                                    <dt>Culture</dt>
                <dd>British</dd>
